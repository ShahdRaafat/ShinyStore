{
  "name": "shiny",
  "version": "1.0.0",
  "description": "Skincare Store ",
  "default": "index.html",
  "scripts": {
    "start": "rm -rf dist && cp -r public dist && parcel index.html",
    "build": "rm -rf dist && cp -r public dist && parcel build index.html --dist-dir ./dist"
  },
  "author": "shahd",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.13.3"
  },
  "extends": "@parcel/config-default",
  "transformers": {
    "*.woff2": [
      "@parcel/transformer-raw"
    ],
    "*.woff": [
      "@parcel/transformer-raw"
    ],
    "*.ttf": [
      "@parcel/transformer-raw"
    ]
  }
}
